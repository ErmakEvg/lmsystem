@using LMPlatform.Models
@using LMPlatform.UI.ViewModels.SubjectModulesViewModel
@model LMPlatform.UI.ViewModels.SubjectViewModels.ModulesDataWorkingViewModel

@if (Model.Module.ModuleType == ModuleType.News)
{
    @Html.Partial("Subjects/Modules/News/_NewsModule", (ModulesNewsViewModel) Model.DataModel)
}
else if (Model.Module.ModuleType == ModuleType.Lectures)
{
    @Html.Partial("Subjects/Modules/Lectures/_LecturesModule", (ModulesLecturesViewModel) Model.DataModel)
}
else if (Model.Module.ModuleType == ModuleType.Labs)
{
    @Html.Partial("Subjects/Modules/Labs/_LabsModule", (ModulesLabsViewModel)Model.DataModel)
}
else if (Model.Module.ModuleType == ModuleType.Practical)
{
    @Html.Partial("Subjects/Modules/Practicals/_PracticalModule", (ModulesLabsViewModel)Model.DataModel)
}