@model LMPlatform.Models.KnowledgeTesting.TestUnlockInfo
<li class="list-group-item">
    <div class="row">
        <div class="col-lg-1">
            @Model.Number
        </div>
        <div class="col-lg-10">
            @Model.StudentName
            @Html.HiddenFor(model => model.StudentId, new { @class = "studentUnlockItem" })
        </div>
        <div class="col-lg-1">
            <a class="personaLockButton" style="margin-right: 10px">
                <span class="@(Model.Unlocked ? "fa fa-unlock fa-2x" : "fa fa-lock fa-2x")" data-student-id='@Model.StudentId' data-unlocked="@Model.Unlocked" />
            </a>
        </div>
    </div>
</li>
